#include<stdio.h>
main()
{
    int weekno;
	scanf("%d",&weekno);
	if(weekno==1)
	{
	printf("sunday");
	}
	else if(weekno==2)
	{
	printf("monday");
	}
	else if(weekno==3)
	{
	printf("tuesday");
	}
	else if(weekno==4)
	{
	printf("wednesday");
	}
	else if(weekno==5)
	{
	printf("thursday");
	}
	else if(weekno==6)
	{
	printf("friday");
	}
	else if(weekno==7)
	{
	printf("saturday");
    }
	else
	{
		printf("none");
	}
}
